                function showSLF() {
                     if( flag == 1) {
                       // Make a request to the handler with new ads 
                       jQuery.post('/gp/product/handler.html', { 'sid': '184-0609371-4558240','jsonAds' : escape(jsonAds) } ,function(data) {
